{
    "swagger": "2.0",
    "info": {
        "description": "This is the API for the EODHP Workspace Services.",
        "title": "EODHP Workspace Services API",
        "contact": {},
        "version": "v1"
    },
    "paths": {
        "/workspaces/{workspace-id}/users/{user-id}/s3-tokens": {
            "post": {
                "description": "Request S3 session credentials for user access to a single workspace. {user-id} can be set to \"me\" to use the token owner's user id.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "s3 credentials auth"
                ],
                "summary": "Request S3 session credentials",
                "parameters": [
                    {
                        "type": "string",
                        "example": "my-workspace",
                        "description": "Workspace ID",
                        "name": "workspace-id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "string",
                        "example": "me",
                        "description": "User ID",
                        "name": "user-id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/handlers.S3Credentials"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "handlers.S3Credentials": {
            "type": "object",
            "properties": {
                "accessKeyId": {
                    "type": "string"
                },
                "expiration": {
                    "type": "string"
                },
                "secretAccessKey": {
                    "type": "string"
                },
                "sessionToken": {
                    "type": "string"
                }
            }
        }
    }
}