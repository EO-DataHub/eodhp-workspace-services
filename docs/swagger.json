                "parameters": [
                    {
                        "type": "string",
                        "example": "my-workspace",
                        "description": "Workspace ID",
                        "name": "workspace-id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "string",
                        "example": "me",
                        "description": "User ID",
                        "name": "user-id",
                        "in": "path",
                        "required": true
                    }
